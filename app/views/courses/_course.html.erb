<div id="<%= dom_id course %>">
  <p>
    <strong>Title:</strong>
    <%= course.title %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= course.description %>
  </p>

  <p>
    <strong>Instructor:</strong>
    <%= course.instructor_id %>
  </p>

  <p>
    <strong>Created at:</strong>
    <%= course.created_at %>
  </p>

  <p>
    <strong>Updated at:</strong>
    <%= course.updated_at %>
  </p>

</div>
